{
  "name": "arie/ovotest",
  "description": "Ovo Test Plugin",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "nttis",
      "email": "email@example.com"
    }
  ],
  "autoload": {
    "classmap": [
      "src"
    ]
  },
  "repositories": [
    {
      "type": "path",
      "url": "d:/pmmp/Ovommand",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "git",
      "url": "https://github.com/GalaxyGamesMC/Ovommand.git"
    }
  ],
  "require": {
    "galaxygames/ovommand": "dev-main"
  },
  "require-dev": {
    "sof3/pharynx": "dev-master"
  },
  "scripts": {
    "build": "php -dphar.readonly=0 vendor/bin/pharynx -i=. -c -v -p=../../plugins/OvoTest.phar"
  }
}
